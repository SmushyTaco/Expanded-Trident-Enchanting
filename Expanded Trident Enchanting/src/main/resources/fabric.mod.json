{
  "schemaVersion": 1,
  "id": "expanded_trident_enchanting",
  "version": "${version}",
  "name": "Expanded Trident Enchanting",
  "description": "This mod allows you to use Fire Aspect, Knockback, Looting, and all other sword enchantment types on a trident.",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/expanded-trident-enchanting/issues/",
    "sources": "https://github.com/SmushyTaco/Expanded-Trident-Enchanting/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/expanded_trident_enchanting/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.expanded_trident_enchanting.ExpandedTridentEnchanting"
      }
    ],
    "modmenu": [
      "com.smushytaco.expanded_trident_enchanting.configuration_support.ModMenuIntegration"
    ]
  },
  "mixins": [
    "expanded_trident_enchanting.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.1+kotlin.1.6.10",
    "minecraft": "1.18.x",
    "cloth-config2": ">=6.1.48",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": ">=3.0.1"
  }
}